{
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "declaration": true,
    "skipLibCheck": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "preserveSymlinks": true,
    "moduleResolution": "node",
    "noEmit": false,
    "lib": ["es2018"],
    "sourceMap": true,
    "declarationMap": true,
    "strict": true,
    "noImplicitReturns": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": false,
    "noErrorTruncation": true,
    "allowJs": false,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "suppressImplicitAnyIndexErrors": true,
    "stripInternal": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@matechs/core/*": ["packages/core/build/*"],
      "@matechs/core": ["packages/core/build"],
      "@matechs/contrib/*": ["packages/contrib/build/*"],
      "@matechs/contrib": ["packages/contrib/build"],
      "@matechs/test/*": ["packages/test/build/*"],
      "@matechs/test": ["packages/test/build"],
      "@matechs/test-jest/*": ["packages/test-jest/build/*"],
      "@matechs/test-jest": ["packages/test-jest/build"],
      "@matechs/aio/*": ["packages/aio/build/*"],
      "@matechs/aio": ["packages/aio/build"],
      "@matechs/graceful/*": ["packages_be/graceful/build/*"],
      "@matechs/graceful": ["packages_be/graceful/build"],
      "@matechs/orm/*": ["packages_be/orm/build/*"],
      "@matechs/orm": ["packages_be/orm/build"],
      "@matechs/tracing/*": ["packages_be/tracing/build/*"],
      "@matechs/tracing": ["packages_be/tracing/build"],
      "@matechs/uuid/*": ["packages_be/uuid/build/*"],
      "@matechs/uuid": ["packages_be/uuid/build"],
      "@matechs/zoo/*": ["packages_be/zoo/build/*"],
      "@matechs/zoo": ["packages_be/zoo/build"],
      "@matechs/browser/*": ["packages_fe/browser/build/*"],
      "@matechs/browser": ["packages_fe/browser/build"],
      "@matechs/rxjs/*": ["packages_fe/rxjs/build/*"],
      "@matechs/rxjs": ["packages_fe/rxjs/build"],
      "@matechs/epics/*": ["packages_fe/epics/build/*"],
      "@matechs/epics": ["packages_fe/epics/build"],
      "@matechs/console/*": ["packages_sys/console/build/*"],
      "@matechs/console": ["packages_sys/console/build"],
      "@matechs/logger/*": ["packages_sys/logger/build/*"],
      "@matechs/logger": ["packages_sys/logger/build"],
      "@matechs/logger-pino/*": ["packages_sys/logger-pino/build/*"],
      "@matechs/logger-pino": ["packages_sys/logger-pino/build"],
      "@matechs/logger-winston/*": ["packages_sys/logger-winston/build/*"],
      "@matechs/logger-winston": ["packages_sys/logger-winston/build"],
      "@matechs/http-client/*": ["packages_http/http-client/build/*"],
      "@matechs/http-client": ["packages_http/http-client/build"],
      "@matechs/http-client-fetch/*": ["packages_http/http-client-fetch/build/*"],
      "@matechs/http-client-fetch": ["packages_http/http-client-fetch/build"],
      "@matechs/http-client-libcurl/*": ["packages_http/http-client-libcurl/build/*"],
      "@matechs/http-client-libcurl": ["packages_http/http-client-libcurl/build"],
      "@matechs/express/*": ["packages_http/express/build/*"],
      "@matechs/express": ["packages_http/express/build"],
      "@matechs/apollo/*": ["packages_http/apollo/build/*"],
      "@matechs/apollo": ["packages_http/apollo/build"],
      "@matechs/koa/*": ["packages_http/koa/build/*"],
      "@matechs/koa": ["packages_http/koa/build"],
      "@matechs/rpc-client/*": ["packages_http/rpc-client/build/*"],
      "@matechs/rpc-client": ["packages_http/rpc-client/build"],
      "@matechs/rpc/*": ["packages_http/rpc/build/*"],
      "@matechs/rpc": ["packages_http/rpc/build"],
      "@matechs/cqrs/*": ["packages_inc/cqrs/build/*"],
      "@matechs/cqrs": ["packages_inc/cqrs/build"],
      "@matechs/fancy/*": ["packages_inc/fancy/build/*"],
      "@matechs/fancy": ["packages_inc/fancy/build"],
      "@matechs/morphic-alg/*": ["packages/morphic-alg/build/*"],
      "@matechs/morphic-alg": ["packages/morphic-alg/build"],
      "@matechs/morphic/*": ["packages/morphic/build/*"],
      "@matechs/morphic": ["packages/morphic/build"],
      "@matechs/react": ["packages/react/build"]
    }
  },
  "exclude": ["node_modules", "build", "lib"]
}
